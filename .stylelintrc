{
  "extends": "stylelint-config-standard",
  "formatter": "stylelint-checkstyle-formatter",
  "plugins": [],
  "rules": {
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": ["composes"]
      }
    ],
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["tailwind"]
      }
    ]
  }
}
